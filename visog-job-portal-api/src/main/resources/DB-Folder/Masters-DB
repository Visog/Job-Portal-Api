
<<<<<------------MASTERS DB TABLES-------->>>>>

1. CREATE TABLE ROLES
	(
	ID VARCHAR(37) NOT NULL,
	NAME VARCHAR (100) NOT NULL,
	DESCRIPTION VARCHAR (250),
	CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID) 
	);

2. CREATE TABLE GENDER
	(
	ID VARCHAR(37) NOT NULL,
	CODE VARCHAR(3) NOT NULL,
	NAME VARCHAR (100) NOT NULL,
	DESCRIPTION VARCHAR (250),
	CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID)
	);

3. CREATE TABLE COUNTRY
	(
	ID VARCHAR(37) NOT NULL,
	CODE VARCHAR(9) NOT NULL,
	NAME VARCHAR(200) NOT NULL,
	CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID)
	); 
	
4.	CREATE TABLE STATE
	(
	ID VARCHAR(37) NOT NULL,
	NAME VARCHAR (200) NOT NULL,
	COUNTRY_ID VARCHAR(37) NOT NULL,
	CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID),
	FOREIGN KEY (COUNTRY_ID) REFERENCES COUNTRY (ID)
	);



5.	CREATE TABLE CITY
	(
	ID VARCHAR(37) NOT NULL,
	NAME VARCHAR (200) NOT NULL,
	STATE_ID VARCHAR(37),
	COUNTRY_ID VARCHAR(37),
	ORDER_NUM INT,
	CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID),
	FOREIGN KEY (STATE_ID) REFERENCES STATES  (ID)	
	);
	
6.	CREATE TABLE ADDRESS_TYPE
	 (
	ID VARCHAR(37) NOT NULL,
	NAME VARCHAR (100) NOT NULL,/* Permanent/Residence */
	DESCRIPTION VARCHAR (500) NOT NULL,
	CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID)
	);	
	
7.	CREATE TABLE COURSES
	( 
	ID VARCHAR(37) NOT NULL,
	NAME VARCHAR (100) NOT NULL,/*Btech/MTech*/
	DESCRIPTION VARCHAR (500) NOT NULL,
    CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID)
	);	
	
	
8.	CREATE TABLE SPECILIAZATION
	( 
	ID VARCHAR(37) NOT NULL,
	NAME VARCHAR (100) NOT NULL, /*IT/Mechanical*/
	DESCRIPTION VARCHAR (500) NOT NULL,
	COURSE_ID VARCHAR(37) NOT NULL,
    CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID), 
	FOREIGN KEY (COURSE_ID) REFERENCES COURSES (ID)
	);	
	
9.	CREATE TABLE UNIVERSITY
	(
	ID  VARCHAR(37) NOT NULL,
	NAME VARCHAR (100) NOT NULL,/*OU/JNTU*/
	DESCRIPTION VARCHAR (500) NOT NULL,
    CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID)
	);	
	
	
10.	CREATE TABLE  EDUCATION_TYPE
	(
	ID VARCHAR(37) NOT NULL,
	NAME VARCHAR (100) NOT NULL, /*Regular/Distance*/
	DESCRIPTION VARCHAR (500) NOT NULL,
	CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID)
	);
	
	
11.	CREATE TABLE LANGUAGES
	( 
	ID VARCHAR(37) NOT NULL,
	CODE VARCHAR (100) NOT NULL,
	NAME VARCHAR (100) NOT NULL,
	CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID)
	);			
	
	
12.	CREATE TABLE CURRENCY
	( 
	ID VARCHAR(37) NOT NULL,
	CODE VARCHAR (10) NOT NULL,
	NAME VARCHAR (100) NOT NULL,
	SYMBOL  VARCHAR(30),
    CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID)
	);	

13.	CREATE TABLE INDUSTRY
	( 
	ID VARCHAR(37) NOT NULL,
	NAME VARCHAR (100) NOT NULL, /**IT/PHARMA**/
	DESCRIPTION VARCHAR (500) NOT NULL,
	CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID)
	);
	
14.	CREATE TABLE DOMAINS
	( 
	ID  VARCHAR(37) NOT NULL,
	NAME VARCHAR (100) NOT NULL,/*IT/PHARMA*/
	DESCRIPTION VARCHAR (500) NOT NULL,
	CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID)
	);	
	
15.	CREATE TABLE JOB_ROLE
	(
	ID VARCHAR(37) NOT NULL,
	NAME VARCHAR (100) NOT NULL,/*--Application design/Developer/System Admin*/
	DESCRIPTION VARCHAR (500) NOT NULL,	
	CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID)
	);	
	
16.	CREATE TABLE STATUS 
	(
	ID VARCHAR(37) NOT NULL,	
	CODE VARCHAR (50) NOT NULL,	
	NAME VARCHAR (100) NOT NULL,-- blocked,active,closed
	CATEGORY VARCHAR (100) NOT NULL,	
	ORDER_NUM INT NOT NULL,	
	DESCRIPTION VARCHAR (500) NOT NULL,	
    CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID)
	);	
	
17.	CREATE TABLE EMPLOYMENT_TYPE 
	 (
	ID VARCHAR(37) NOT NULL,
	NAME VARCHAR (100) NOT NULL,-- full time/part time/contractual project
	DESCRIPTION VARCHAR (500) NOT NULL,	
	CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID)
	);	
	
18.	CREATE TABLE EMPLOYER_TYPE 
	 (
	ID VARCHAR(37) NOT NULL,
	NAME VARCHAR (100) NOT NULL,-- HR /CONSULTANT
	DESCRIPTION VARCHAR (500) NOT NULL,	
	CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID)
	);
	
19.	CREATE TABLE FILE_TYPE 
	(
	ID VARCHAR(37) NOT NULL,
	EXTENSION VARCHAR(50) NOT NULL,-- txt,pdf,mp4,mp3
	DESCRIPTION VARCHAR (200) NOT NULL,
    CREATED_SID   VARCHAR(37) NOT NULL,
	CREATED_BY    VARCHAR(37) NOT NULL,
	CREATED_ON    TIMESTAMP NOT NULL,
	UPDATED_SID   VARCHAR(37),
	UPDATED_BY    VARCHAR(37),
	UPDATED_ON    TIMESTAMP,
	PRIMARY KEY (ID)
	);
	
	